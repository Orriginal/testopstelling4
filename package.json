{
    "name": "aphrodite-lib-stencil",
    "version": "0.2.2",
    "private": false,
    "description": "Aphrodite component library",
    "main": "dist/index.cjs.js",
    "module": "dist/custom-elements/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/custom-elements/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/aphrodite-lib-stencil/aphrodite-lib-stencil.esm.js",
    "files": [
        "dist/",
        "loader/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "docs": "stencil build --docs-readme",
        "storybook.run": "start-storybook -p 6006 -s ./public",
        "build.watch": "stencil build - - watch",
        "dev": "concurrently \"npm:build.watch\" \"npm:storybook\"",
        "storybook": "start-storybook -p 6006 -s ./public --no-manager-cache",
        "build-storybook": "build-storybook",
    },
    "dependencies": {
        "@stencil/core": "^2.6.0",
        "prettier": "^2.2.1"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.13.1",
        "@rollup/plugin-url": "^6.0.0",
        "@stencil/postcss": "^2.0.0",
        "@stencil/sass": "^1.4.1",
        "@storybook/addon-actions": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-knobs": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/html": "^6.2.9",
        "@types/autoprefixer": "^10.2.0",
        "@types/jest": "^26.0.20",
        "@types/puppeteer": "^5.4.2",
        "@whitespace/storybook-addon-html": "^5.0.0",
        "autoprefixer": "^10.2.4",
        "babel-loader": "^8.2.2",
        "case": "^1.6.3",
        "hammerjs": "^2.0.8",
        "jest": "^26.6.3",
        "jest-cli": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "puppeteer": "^5.5.0",
        "storybook-addon-designs": "^6.0.0",
        "typescript": "^4.3.2",
        "ts-node": "^10.0.0"
        "stencil-tailwind": "^0.0.6",
    }
}
